function unpack(packed)
 unpacked={}
 for i=1,#packed,2 do
  local num = packed[i]
  local val = packed[i+1]
  for j=1,num do
   add(unpacked,val)
  end
 end
 return unpacked
end

function create_bomb()
 local entity = {
  sprites={23,24,25,26,27,28},
  anims={
   -- dir: 1=loop,2=once
   --idle={dir=1,frames=unpack({25,1, 4,2, 40,4, 4,5, 3,3})},
   idle={dir=1,frames=unpack({5,1, 5,2, 7,3, 7,4, 5,5})},
  },
  f=0, -- current anim frame index. 0-based.
  anim="idle",
  
  play_anim = function(self,a)
   self.anim = a
   self.f = 0
  end,
  
  update = function(self)
   local a = self.anims[self.anim]
   local nb_frames = #a.frames
   self.f = 
    (a.dir == 1) 
    and ( self.f + 1 ) % nb_frames 
    or max(nb_frames-1,self.t+1)
  end,
  
  draw = function(self)
   local sprite = self.sprites[self.anims[self.anim].frames[self.f+1]] -- to 1-based
   palt(3,true)
   palt(0,false)
   spr(sprite,16,48)
   pal()
  end
 }
 
 return entity
end

function create_dude(base_spr)
 local entity = {
  sprites={base_spr,base_spr+1,base_spr+2},
  anims={
   -- dir: 1=loop,2=once
   idle={dir=1,frames=unpack({25,2,25,3})},
   walk={dir=1,frames=unpack({15,1,8,2,15,3,8,2})},
   run1={dir=1,frames=unpack({10,1,8,2,10,3,8,2})},
   run2={dir=1,frames=unpack({8,1,8,2,10,3,8,2})},
   run3={dir=1,frames=unpack({5,1,8,2,10,3,8,2})},
   death={dir=2,frames=unpack({10,1,8,2,10,3,8,2})}
  },
  f=0, -- current anim frame index. 0-based.
  anim="idle",
  
  play_anim = function(self,a)
   self.anim = a
   self.f = 0
  end,
  
  update = function(self)
   local a = self.anims[self.anim]
   local nb_frames = #a.frames
   self.f = 
    (a.dir == 1) 
    and ( self.f + 1 ) % nb_frames 
    or max(nb_frames-1,self.t+1)
  end,
  
  draw = function(self,x)
   local sprite = self.sprites[self.anims[self.anim].frames[self.f+1]] -- to 1-based
   spr(sprite,x,64)
   spr(sprite+16,x,64+8)
  end
 }
 
 return entity
end

