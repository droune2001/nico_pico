pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
perso={
 x=64,
 y=64,
 mir=false,
 anim="talk",
 walk={
  f=0, -- frame
  st=3,-- start in sprite sheet
  sz=2,-- nb of anim frames
  szp=2, -- size of sprite
  spd=1/15 -- speed
  },
 talk={f=0,st=32,sz=3,szp=2,spd=1/5}
}

ctime = {
 m = 0,
 s = 0,
 ms = 0
}

particles = {}
explosions = {}

function make_explosion(x,y,nb)
 while ( nb > 0 ) do
  explo = {}
  explo.x = x+(rnd(2)-1)*10
  explo.y = y+(rnd(2)-1)*10
  explo.r = 4+(rnd(2)-1)*10
  explo.c = 10 -- jaune
  add(explosions,explo)
  sfx(0)
  nb -= 1
 end
end

function make_particles(nb)
 while ( nb > 0 ) do
  part = {}
  part.x = 64 + 8 --perso.x + 4
  part.y = 64 + 8 --perso.y + 4
  part.col = flr(rnd(16))
  part.dx = rnd(2) - 1
  part.dy = rnd(2) - 1
  part.f = 0
  part.maxf = 30*3
  add(particles,part)
  nb -= 1
 end
end

function draw_explosions()
 for e in all(explosions) do
  circfill(e.x,e.y,e.r,e.c)
  -- update
  e.r -= 1
  if (e.r <= 4) e.c = 9
  if (e.r <= 2) e.c = 8
  if (e.r <= 0) del(explosions,e)
 end
end

function draw_particles()
 for part in all(particles) do
  pset(part.x, part.y, part.col)
  -- update
  part.x += part.dx
  part.y += part.dy
  part.f += 1
  if(part.f > part.maxf or
     part.x < 0 or --part.x < perso.x - 64 or
     part.y < 0 or --part.y < perso.y - 64 or 
     part.x >= 128 or --part.x >= perso.x + 64 or
     part.y >= 128 --part.y >= perso.y + 64
     ) then
    del(particles, part)
  end
 end
end

function anim(a) -- passed by ref
 a.f += a.spd -- accum speed
 if (a.f >= a.sz) then
  a.f -= a.sz -- loop
 end
 return a.st + a.szp * flr(a.f) -- floor
end

function anim_player()
 if(perso.anim=="talk") then
  return anim(perso.talk)
 else
  return anim(perso.walk)
 end
end

function highlight(f,x,y,col)
 -- replace all palette 
 -- colors with col
 for i=1,16 do
  pal(i,col)
 end
 spr(f,x,y+1)
 spr(f,x,y-1)
 spr(f,x+1,y)
 spr(f,x-1,y)
 pal() -- reinit palette
 spr(f,x,y)
end

function shadow(f,x,y,col)
 -- replace all palette 
 -- colors with col
 for i=1,16 do
  pal(i,col)
 end
 spr(f,x,y+1)
 spr(f,x+1,y)
 pal() -- reinit palette
 spr(f,x,y)
end

function controls()
 perso.anim="talk"
 if(btn(0)) then
  perso.x -= 1
  perso.mir=true
 end
 if(btn(1)) then
  perso.x += 1;
  perso.mir=false
 end
 if(btn(2)) then
  perso.y -= 1;
 end
 if(btn(3)) then
  perso.y += 1;
 end
 if(btn(0) or btn(1) or
    btn(2) or btn(3)) then
  perso.anim="walk"
 end
 if(btn(4)) make_particles(50)
 if(btnp(5)) make_explosion(64+8,64+8,15)
end

function time_manager()
 ctime.ms += 1/30 -- 30hz update
 if(ctime.ms >= 1) then
  ctime.s += 1
  ctime.ms -= 1
  if(ctime.s >= 60) then
   ctime.m += 1
   ctime.s = 0
  end
 end
end

-- called at 30hz
function _update()
 controls()
 time_manager()
end

function _draw()
 cls()
 --for i=0,128 do
  --pset(i,10,8)
 --end
 map(0,0,64-perso.x,64-perso.y,64,32)
--spr(003,perso.x,perso.y,2,2,perso.mir)
--spr(003,64,64,2,2,perso.mir)
 spr(anim_player(),64,64,2,2,perso.mir)
 highlight(16,100,1,7)
 shadow(16,108,1,2)
 draw_particles()
 draw_explosions()
 print(ctime.s)
end

